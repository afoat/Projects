      <sectionGroup name="Foat.Puzzles.ConfigSection">
        <section name="ParallelIDAStar" type="Foat.Puzzles.Configuration.ParallelIDAStarSettings, Foat.Puzzles" />
      </sectionGroup>
    </configSections>
    <startup> 
